@font-face {
	font-family: 'icomoon';
	src:url('fonts/icomoon.eot?-wtw11y');
	src:url('fonts/icomoon.eot?#iefix-wtw11y') format('embedded-opentype'),
		url('fonts/icomoon.woff?-wtw11y') format('woff'),
		url('fonts/icomoon.ttf?-wtw11y') format('truetype'),
		url('fonts/icomoon.svg?-wtw11y#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-tag:before {
	content: "\f02b";
}
.icon-advertising:before {
	content: "\e600";
}
.icon-strategy:before {
	content: "\e601";
}
.icon-consulting:before {
	content: "\e602";
}
.icon-arrow-left:before {
	content: "\e603";
}
