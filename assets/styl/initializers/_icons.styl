@font-face {
	font-family: 'icomoon';
	src:url('fonts/icomoon.eot?-khsapn');
	src:url('fonts/icomoon.eot?#iefix-khsapn') format('embedded-opentype'),
		url('fonts/icomoon.woff?-khsapn') format('woff'),
		url('fonts/icomoon.ttf?-khsapn') format('truetype'),
		url('fonts/icomoon.svg?-khsapn#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

